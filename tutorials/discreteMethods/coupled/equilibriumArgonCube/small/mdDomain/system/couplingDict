FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      couplingDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

couplingName	mdDomain; //Coupling domain name
refLength	3.4e-10;  //Reference length for anything used in the coupling interfaces
refTime		2e-12;    //Reference time for anything used in the coupling interfaces

couplingConfigurations
( 
    TwoDInterfaces //2D coupling interfaces
    {
    }

    ThreeDInterfaces //3D coupling interfaces
    {
	ifs_1
	{	
	    sending			yes;  
	    receiving			yes;
            smartSend			yes;
	    iterationCoupling		yes;    
	}

	ifs_2
	{
	    sending			yes;	
	    receiving			yes;	
            smartSend			yes;  
	    iterationCoupling		yes;   
	}
    }
);

