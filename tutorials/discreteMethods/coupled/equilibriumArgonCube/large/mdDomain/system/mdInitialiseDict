FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      mdInitialiseDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

polyConfigurations
(
    configuration
    {
        type		polyBCC;
	SIUnits		yes;
        temperature	300;
        bulkVelocity	(0 0 0);
        molId		Ar;
        frozen		no;
    
        boundBox
        {
          startPoint	(1e-6 0 0);
          endPoint	(1.5e-6 5e-7 5e-7);  
        }
    
        unitCellSize	3;
        N		3021000;
    }
);

